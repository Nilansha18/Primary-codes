#include <bits/stdc++.h>
using namespace std;

int maxval(vector<int>arr){
    int m=arr[0];
    for (int i=0; i<arr.size(); i++){
        if(arr[i]>m) m=arr[i];
    }
    return m;
}

void CountSort(vector<int>arr){
    int max=maxval(arr);
    vector<int>count(max+1 , 0);
    for(int i=0; i<(max+1) ;i++){
        count[arr[i]]++;
    }

    for(int i=1; i<(max+1); i++){
        count[i]+=count[i-1];
    }
    vector<int>k;
    for(int i=(arr.size() -1); i>=0;i-- ){
        k[--count[arr[i]]]=arr[i];
    }
    for (int i=0; i<arr.size(); i++){
        arr[i]=k[i];
    }

}

int main(){
    int n,a, l,r;
    vector<int> arr;
    cout<<"Enter the number of elements :";
    cin>>n;
    for(int i=0; i<n; i++){
        cin>>a;
        arr.push_back(a);
    }
    
    CountSort(arr);
    for(int i=0; i<n; i++){
        cout<<arr[i]<<" ";
    }
    return 0;
}
